{
  "name": "jquery-common-keydown",
  "description": "jQuery collection plugin that triggers events for common accessibility keys e.g. ENTER, SPACE, ESC, ARROW KEYS.",
  "version": "0.3.0",
  "scripts": {
    "start": "karma start --autoWatch true --singleRun false",
    "test": "karma start --autoWatch false --singleRun true",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "build": "npm run jsdoc && npm run minify",
    "clean": "rimraf jsdoc test_reports *.min.js lint.txt",
    "fix": "eslint jquery.commonkeydown.js --fix",
    "jsdoc": "jsdoc jquery.commonkeydown.js -d jsdoc",
    "lint": "eslint jquery.commonkeydown.js > lint.txt",
    "minify": "jsmin -o jquery.commonkeydown.min.js jquery.commonkeydown.js"
  },
  "pre-commit": "prepublish",
  "author": "Ian McBurnie <ianmcburnie@hotmail.com>",
  "homepage": "https://github.com/ianmcburnie/jquery-common-keydown",
  "bugs": {
    "url": "https://github.com/ianmcburnie/jquery-common-keydown/issues"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "ebay",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcburnie/jquery-common-keydown.git"
  },
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2",
    "eslint": "^3",
    "eslint-config-google": "~0.6",
    "jasmine-core": "^2",
    "jquery": ">=1.11",
    "jquery-migrate": "^3",
    "jsdoc": "^3",
    "jsmin": "^1",
    "karma": "^1",
    "karma-coverage": "^1",
    "karma-html-reporter": "~0.2",
    "karma-jasmine": "^1",
    "karma-junit-reporter": "^1",
    "karma-phantomjs-launcher": "^1",
    "phantomjs-prebuilt": "^2",
    "pre-commit": "^1",
    "rimraf": "^2"
  },
  "peerDependencies": {
    "jquery": ">=1.11"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
